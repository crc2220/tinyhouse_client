{
    "name": "tinyhouse-v1-client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@types/react": "^16.9.56",
        "@types/react-dom": "^16.9.9",
        "@types/react-router-dom": "^5.1.6",
        "antd": "^4.1.4",
        "apollo": "^2.31.1",
        "apollo-boost": "^0.4.9",
        "cra-template-typescript": "1.1.0",
        "graphql": "^15.4.0",
        "react": "^17.0.1",
        "react-apollo": "^3.1.5",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.0",
        "typescript": "^4.0.5"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "codegen:schema": "npx apollo client:download-schema --endpoint=http://localhost:9000/api",
        "codegen:generate": "npx apollo client:codegen --localSchemaFile=schema.json --includes=src/**/*.ts --globalTypesFile=./src/lib/graphql/globalTypes.ts --target=typescript"
    },
    "eslintConfig": {
        "extends": "react-app",
        "rules": {
            "additional-rule": "warn"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "http://localhost:9000",
    "devDependencies": {
        "@types/graphql": "^14.5.0",
        "eslint-plugin-react-hooks": "^4.2.0"
    }
}
